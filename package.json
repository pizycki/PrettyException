{
    "name": "pretty-exception",
    "displayName": "Pretty Exception",
    "description": "Visual Studio Code extension for formatting exceptions returned by API serialized with JSON.",
    "version": "0.0.1",
    "publisher": "pizycki",
    "repository": {
        "type": "git",
        "url": "https://github.com/pizycki/PrettyException"
    },
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Pretty exception"
        }]
    },
    "scripts": {
        "vscode:prepublish": "",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}